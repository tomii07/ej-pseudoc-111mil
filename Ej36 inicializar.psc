Proceso inicializar
		Definir lista,val Como caracter;
		Definir i Como Entero;
		Dimension lista[5];
		val <- 'Hola mundo en lista';
		i <- 0;
		cargar_lista(lista,i,val);
		Escribir "LISTA: ";
		mostrar(lista,i);
FinProceso
//------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//
Funcion cargar_lista(lista,i,val)
	Repetir
		lista[i] <- val;
		i <- i + 1;
	Hasta Que i = 5;	
FinFuncion
//------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//
Funcion mostrar(lista,i)
	Repetir
		Escribir "[",lista[i],"]";
		i <- i + 1;
	Hasta Que i = 5
	Escribir "";
FinFuncion
//------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//
